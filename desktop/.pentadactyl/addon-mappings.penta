command fb -description "Toggle Firebug" emenu View.Firebug
command fbd -description "Disable Firebug" firebug disable

command wd -description "Toggle Web Developer Toolbar" toolbartoggle Web Developer Toolbar
toolbarhide Web Developer Toolbar

map <A-c> :set noscript-objects!=<A-Tab>
map <A-s> :set noscript-sites!=<A-Tab>
map <A-S-s> :set noscript-tempsites!=<A-Tab>
map <A-C-s> :set noscript-untrusted!=<A-Tab>
